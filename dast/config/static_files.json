{
    "extensions": [
        ".css", ".png", ".jpg", ".jpeg", ".gif", ".svg", ".ico",
        ".woff", ".woff2", ".ttf", ".eot", ".otf",
        ".mp4", ".mp3", ".wav", ".avi", ".mov", ".wmv", ".flv", ".mkv",
        ".webp", ".bmp", ".tiff", ".tif",
        ".map", ".txt", ".xml", ".swf", ".webm", ".webmanifest",
